{
    "host": "0.0.0.0",
    "port": 8000,
    "log_level": "info",
    "database": {
        "type": "memory",
        "path": "data/dispatcher.db"
    },
    "task_assignment": {
        "strategy": "least_loaded",
        "max_retries": 3,
        "retry_delay": 300
    },
    "worker_management": {
        "heartbeat_interval": 30,
        "heartbeat_timeout": 90,
        "auto_remove_offline": true,
        "offline_threshold": 300
    },
    "security": {
        "api_key_required": false,
        "api_keys": []
    }
}